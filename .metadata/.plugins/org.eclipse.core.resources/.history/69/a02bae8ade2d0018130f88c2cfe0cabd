#include <msp430.h> 

void sq_ware(void);
/**
 * main.c
 */
int main(void)
{
	WDTCTL = WDTPW | WDTHOLD;	// stop watchdog timer
	PM5CTL0 &= ~LOCKLPM5;

	sq_ware();
	
	return 0;
}


void sq_ware(void){
    P1DIR |= BIT0;
    P1OUT |= 0x01;
    _delay_cycles(1000);

    P1OUT &= ~0x01;
    _delay_cycles(1000);

}
