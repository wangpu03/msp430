#include <msp430.h> 

/**
 * main.c
 */
int main(void)
{
	WDTCTL = WDTPW | WDTHOLD;	// stop watchdog timer
	PM5CTL0 &= ~LOCKLPM5;

	sq_ware();
	
	return 0;
}


void sq_ware(){
    P1DIR |= BIT0;

    while(1){
        P1OUT |= 0x01;
        _delay_cycles(1000);

        P1OUT &= ~0x01;
        _delay_cycles(1000);
    }
}
