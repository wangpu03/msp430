#include <msp430.h> 


/**
 * main.c
 * 仿真WISP闪灯模式，利用中断技巧
 */
int main(void)
{
	WDTCTL = WDTPW | WDTHOLD;	            // stop watchdog timer
    PM5CTL0 &= ~LOCKLPM5;                   // Disable the GPIO power-on default high-impedance mode

    P4DIR |= BIT0;                          // Set P4.0 to output direction，LED1
    P4OUT |= BIT0;


	return 0;
}
